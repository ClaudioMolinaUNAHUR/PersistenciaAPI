     mkdir api_v1
     cd api_v1/
     npm install --save sequelize
     npm init -y
     sudo apt-get install mariadb
     sudo apt-get install mariadb-server
     npm install --save mariadb
     sudo npm install express-generator -g
     npm install 
     npm install sequelize --save
     npm install sequelize-cli --save
     npx sequelize init
     DEBUG=api:* npm start

     // cache de peticiones 
     npm install express-expeditious --save

     npx sequelize-cli model:generate --name carrera --attributes nombre:string
     npx sequelize db:migrate

     npx sequelize-cli model:generate --name materia --attributes nombre:string,id_carrera:integer
     set DEBUG=api:* | npm start

     npx sequelize-cli model:generate --name alumno --attributes nombre:string,apellido:string,dni:integer,id_carrera:integer
     npx sequelize-cli model:generate --name profesor --attributes nombre:string,apellido:string,dni:integer,id_materia:integer
     npx sequelize-cli model:generate --name aula --attributes id_materia:integer
     npx sequelize-cli model:generate --name nota --attributes nota:integer,id_alumno:integer,id_profesor:integer
     npx sequelize-cli model:generate --name materia --attributes nombre:string
     npx sequelize-cli model:generate --name planesestudio --attributes id_carrera:integer,id_materia:integer


     npx sequelize-cli model:generate --name user --attributes username:string,email:string,password:string

